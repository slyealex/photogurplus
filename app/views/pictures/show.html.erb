<div class="container">
  <h1>A closer look</h1>

  <p><%= @picture.title %> | <%= @picture.artist %></p>
  <%= image_tag @picture.url %>


  <%= link_to "Back", pictures_path %>
  <%= link_to "Edit", edit_picture_path %>
  <%= link_to "Delete", @picture, method: :delete, data: {confirm: "Are you sure you want to delete this photo?" } %> 


</div>
